{
  "default_task_dx_attributes" : {
    "runSpec": {
      "restartableEntryPoints": "all",
      "executionPolicy": {
        "restartOn": {
          "*": 1
        }
      },
      "timeoutPolicy": {
        "*": {
          "hours": 1
        }
      },
      "access" : {
        "project": "VIEW",
        "network": [
          "*"
        ]
      }
    }
  },

  "per_task_dx_attributes" : {
    "Add": {
      "runSpec": {
        "timeoutPolicy": {
          "*": {
            "minutes": 30
          }
        }
      }
    },
    "Inc" : {
      "runSpec": {
        "timeoutPolicy": {
          "*": {
            "minutes": 30
          }
        },
        "access" : {
          "project": "UPLOAD"
        }
      }
    }
  }
}
